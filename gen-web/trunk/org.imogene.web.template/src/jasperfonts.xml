<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

		<!-- Font for Georgian caracters -->
	   <bean id="FontSylvaen" class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">
	       <property name="name" value="Sylfaen"/>
	       <property name="normal" value="org/imogene/web/server/fonts/sylfaen.TTF"/>
	       <property name="pdfEncoding" value="Identity-H"/>
	       <property name="pdfEmbedded" value="true"/>
	   </bean>

</beans>


