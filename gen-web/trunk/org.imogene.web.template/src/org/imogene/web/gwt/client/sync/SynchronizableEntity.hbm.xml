<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class lazy="false" table="sync_entity" name="org.imogene.web.gwt.client.sync.SynchronizableEntity">	
		<id column="SyncEntity_Id" name="id"/>
		<property column="SyncEntity_LastModificationDate" name="modified" type="timestamp"/>
		<property column="SyncEntity_UploadDate" name="uploadDate" type="timestamp"/>
		<property column="SyncEntity_Modifier" name="modifiedBy"/>
		<property column="SyncEntity_ModifiedFrom" name="modifiedFrom"/>	
		<property column="SyncEntity_CreationDate" name="created" type="timestamp"/>	
		<property column="SyncEntity_Creator" name="createdBy"/>	
		<property column="SyncEntity_Name" name="name"/>		
	</class>
	
</hibernate-mapping>