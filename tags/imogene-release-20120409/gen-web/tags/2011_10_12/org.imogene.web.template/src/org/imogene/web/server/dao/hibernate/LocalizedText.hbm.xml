<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class lazy="false" table="LocalizedText" name="org.imogene.web.gwt.common.entity.LocalizedText">
		
		<id name="id" column="LocalizedText_ID"/>			
		<property name="fieldId" column="LocalizedText_FieldId"/>
		<property name="locale" column="LocalizedText_Locale"/>
		<property name="value" column="LocalizedText_Value" type="text"/>
		<property column="LocalizedText_Creator" name="creator"/>
		<property column="LocalizedText_CreationDate" name="creationDate" type="timestamp"/>
		<property column="LocalizedText_Modifier" name="modifier"/>
		<property column="LocalizedText_ModifiedFrom" name="modifiedFrom"/>
		<property column="LocalizedText_LastModificationDate" name="lastModificationDate" type="timestamp"/>
		<property column="LocalizedText_UploadDate" name="uploadDate" type="timestamp"/>		
		
		
						
    </class>		
</hibernate-mapping>
