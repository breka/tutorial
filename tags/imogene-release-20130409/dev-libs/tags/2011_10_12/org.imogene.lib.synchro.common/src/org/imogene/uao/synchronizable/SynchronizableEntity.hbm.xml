<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class lazy="false" table="sync_entity" name="org.imogene.uao.synchronizable.SynchronizableEntity">
	
		<id column="SynchronizableEntity_Id" name="id"/>
		<property column="SynchronizableEntity_LastModificationDate" name="modified" type="timestamp"/>
		<property column="SynchronizableEntity_UploadDate" name="uploadDate" type="timestamp"/>
		<property column="SynchronizableEntity_Modifier" name="modifiedBy"/>
		<property column="SynchronizableEntity_ModifiedFrom" name="modifiedFrom"/>	
		<property column="SynchronizableEntity_CreationDate" name="created" type="timestamp"/>	
		<property column="SynchronizableEntity_Creator" name="createdBy"/>	
		<property column="SynchronizableEntity_Name" name="name"/>

		
	</class>
	
</hibernate-mapping>