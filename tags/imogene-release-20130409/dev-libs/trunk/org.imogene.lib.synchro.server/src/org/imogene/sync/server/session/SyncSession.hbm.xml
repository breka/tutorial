<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- Mapping of the class SyncHistory-->
	<class name="org.imogene.sync.server.session.SyncSession" table="sync_session">
		<id name="id" column="id"/>	
		<property name="initDate" column="init_date" type="timestamp"/>
		<property name="terminalId" column="terminal_id"/>
		<property name="sendDate" column="send_date" type="timestamp"/>
		<property name="type" column="type"/>
		<property name="userId" column="user_id"/>
		<property name="userType" column="user_type"/>
	</class>
</hibernate-mapping>