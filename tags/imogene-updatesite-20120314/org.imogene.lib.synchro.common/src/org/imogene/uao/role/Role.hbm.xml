<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class lazy="false" table="Role" name="org.imogene.uao.role.Role">
	
		<id column="Role_Id" name="id"/>
		<property column="Role_LastModificationDate" name="modified" type="timestamp"/>
		<property column="Role_UploadDate" name="uploadDate" type="timestamp"/>
		<property column="Role_Modifier" name="modifiedBy"/>	
		<property column="Role_ModifiedFrom" name="modifiedFrom"/>	
		<property column="Role_CreationDate" name="created" type="timestamp"/>	
		<property column="Role_Creator" name="createdBy"/>
		<property column="Role_Name" name="name"/>
		
	</class>
	
</hibernate-mapping>