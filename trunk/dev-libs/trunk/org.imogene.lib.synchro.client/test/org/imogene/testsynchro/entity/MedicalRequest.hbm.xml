<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class lazy="false" name="org.imogene.testsynchro.entity.MedicalRequest" table="MedicalRequest">
		<id column="MedicalRequest_ID" name="id"/>			
				
		<many-to-one column="medicalRequestPatient_ID" lazy="false" name="medicalRequestPatient" not-null="false"/>
				
		<many-to-one column="medicalRequestDoctor_ID" lazy="false" name="medicalRequestDoctor" not-null="false"/>
				
		<many-to-one column="medicalRequestMedicalCenter_ID" lazy="false" name="medicalRequestMedicalCenter" not-null="false"/>
		<property column="MedicalRequest_Context" name="context"/>
		<property column="MedicalRequest_Request" name="request"/>
		<property column="MedicalRequest_Photo" name="photo"/>
		<property column="MedicalRequest_Creator" name="createdBy"/>
		<property column="MedicalRequest_CreationDate" name="created" type="timestamp"/>
		<property column="MedicalRequest_Modifier" name="modifiedBy"/>
		<property column="MedicalRequest_ModifiedFrom" name="modifiedFrom"/>
		<property column="MedicalRequest_LastModificationDate" name="modified" type="timestamp"/>
		<property column="MedicalRequest_UploadDate" name="uploadDate" type="timestamp"/>
		
		
						
    </class>		
</hibernate-mapping>
