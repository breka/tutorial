<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.imogene.admin.contrib.specificWorkflow" name="Specific Workflow" schema="schema/org.imogene.admin.contrib.specificWorkflow.exsd"/>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.imogene.admin.contrib.objectContribution1"
            objectClass="org.eclipse.core.resources.IProject">
         <action
               class="org.imogene.admin.contrib.WebGenerationAction"
               icon="icons/web_16.png"
               id="org.imogene.admin.contrib.WebGenerationAction"
               label="Admin application generation"
               menubarPath="imogene.gen.menu/generator">
         </action>
      </objectContribution>
   </extension>
   <extension
         id="org.imogene.admin.contrib.builder.GWTJarBuilder"
         name="GWT jar import"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.imogene.admin.contrib.builder.GWTJarBuilder">
         </run>
      </builder>
   </extension>
	<extension
         id="org.imogene.admin.contrib.builder.CompatibilityBuilder"
         name="Compatibility"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.imogene.admin.contrib.builder.CompatibilityBuilder">
         </run>
      </builder>
   </extension>
</plugin>
