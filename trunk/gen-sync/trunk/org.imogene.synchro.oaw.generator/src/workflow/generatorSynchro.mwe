		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>
	
	<!-- entity helper -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::syncEntityHelper::generate("${packageName}", "${packagePath}", "${modelName}") FOR project' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>
	
	
	<!-- Short name helper -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
