		<genPath value="${home-gen}/src" />	
		<beautifier class="org.eclipse.xpand2.output.XmlBeautifier" />	
	</component>
	
	<!-- spring xml converter-context -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::config::converter_context_sync_xml::generate("${packageName}","${packagePath}") FOR project' />
		<genPath value="${home-gen}/src" />	
		<beautifier class="org.eclipse.xpand2.output.XmlBeautifier" />	
	</component>

    <!-- web.xml for synchro servlet -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::config::web_sync_xml::generate("${packageName}","${projectName}") FOR project'/>
		<genPath value="${home-gen}/WebContent/WEB-INF" />
		<beautifier class="org.eclipse.xpand2.output.XmlBeautifier" />	
	</component>
	
	<!-- application-servlet.xml -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::config::synchro_servlet_xml::generate("${packageName}","${projectName}") FOR project' />
		<genPath value="${home-gen}/WebContent/WEB-INF" />
		<beautifier class="org.eclipse.xpand2.output.XmlBeautifier" />
	</component>	
	
	<!-- jdbc.properties -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::config::jdbc_properties_sync::generate("${JDBCDRIVER}","${JDBCPATH}","${JDBCLOGIN}","${JDBCPASSWORD}", "${HIBERNATEDIALECT}","${SMSSERVER}","${BINARIESPATH}","${videoConverter}","${audioConverter}") FOR project' />
		<genPath value="${home-gen}/src" />		
	</component>
	
	<!-- log4j properties file for synchro servlet 
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::config::log4j_properties_sync::generate("${projectName}","${gen-project-name}") FOR project'/>
		<genPath value="${home-gen}/src" />
	</component>	 -->
	
	<!-- ======================================================================================================================== -->
