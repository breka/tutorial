		</metaModel>
		<expand value='template::uao::default_policy_sync::generate("${packageName}", "${packagePath}") FOR project'/>
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>	
	
	<!-- ======================================================================================================================== -->
	<!-- ===================================================== Localized txts part =============================== -->
	<!-- ======================================================================================================================== -->
	
	
	<component class="org.eclipse.xpand2.Generator" id="javaGenerator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel"><metaModelFile value="${imogeneMetaModel}" /></metaModel>
		<expand value='template::entity::localizedFieldMap_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOR project' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>	
	
	<component class="org.eclipse.xpand2.Generator" id="javaGenerator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel"><metaModelFile value="${imogeneMetaModel}" /></metaModel>
		<expand value='template::entity::localizedFieldMapConverter_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOR project' />
