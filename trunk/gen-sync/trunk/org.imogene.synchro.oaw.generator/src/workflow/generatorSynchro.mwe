	</component>	
	
	<!-- Entity Data Handler implementation -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::entity_handler_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOREACH project.entities' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>	
	
	<!-- Entity Stream Serializer implementation -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::entity_serialize_stream_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOREACH project.entities' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>		
	
	<!-- User Stream Serializer implementation  -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::user_serialize_stream_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOREACH project.entities' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>
	
	<!-- User XML Serialization Converter implementation  -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::user_serialize_filterfield_xml_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOREACH project.entities' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component>	
	
	<!-- User XML Serialization Converter implementation 
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::entity::user_serialize_xml_sync::generate("${packageName}", "${packagePath}", "${modelName}") FOREACH project.entities' />
		<genPath value="${home-gen}/src" />
		<beautifier class="org.eclipse.xpand2.output.JavaBeautifier" />
	</component> -->
	

	<!-- ======================================================================================================================== -->
	<!-- ===================================================== UAO files part =============================== -->
	<!-- ======================================================================================================================== -->
		
	<!-- User access control -->
	<component class="org.eclipse.xpand2.Generator">
		<fileEncoding value="iso-8859-1"/>
		<metaModel class="org.eclipse.xtend.typesystem.emf.EmfMetaModel">
			<metaModelFile value="${imogeneMetaModel}" />
		</metaModel>
		<expand value='template::uao::user_access_sync::generate("${packageName}", "${packagePath}") FOR project'/>
